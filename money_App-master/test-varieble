enter_costname.forEach((item, i) => {
        let enter = document.querySelector(".mainCosts-list"),
            htmlCode = '<li><span class="nameCosts">text</span><span class="forCloseIcon">Сума</span></li>';
        enter.insertAdjacentHTML('beforeEnd', htmlCode);

        let costs = document.querySelector(".nameCosts");
        costs.innerText = item.value

        let forCloseIcon = document.querySelector(".forCloseIcon");
        forCloseIcon.innerText = enter_costsum[i].value;
        console.log(i);
    });

    parentElem.appendChild(elem)